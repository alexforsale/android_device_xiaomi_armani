+#if IST30XX_EXTEND_COORD
+static int check_valid_coord(u32 *msg, int cnt)
+{
+	int ret = 0;
+	u8 *buf = (u8 *)msg;
+	u8 chksum1 = msg[0] >> 24;
+	u8 chksum2 = 0;
+
+	msg[0] &= 0x00FFFFFF;
+
+	cnt *= IST30XX_DATA_LEN;
+
+	while (cnt--)
+		chksum2 += *buf++;
+
+	if (chksum1 != chksum2) {
+		tsp_err("intr chksum: %02x, %02x\n", chksum1, chksum2);
+		ret = -EPERM;
+	}
+
+	return (chksum1 == chksum2) ? 0 : -EPERM;
+}
+#endif
