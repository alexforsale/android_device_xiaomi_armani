diff --git kernel/arch/arm/Kconfig kernel/arch/arm/Kconfig
index 9468df5..4e246e4 100644
--- kernel/arch/arm/Kconfig
+++ kernel/arch/arm/Kconfig
@@ -2197,6 +2197,13 @@ config CMDLINE_FORCE
 	  command-line options your boot loader passes to the kernel.
 endchoice
 
+config BOOT_INFO
